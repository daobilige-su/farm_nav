<?xml version="1.0"?>
<launch>
  <node pkg="farm_nav" type="fws_model.py" name="fws_model"/>

    <!--   move_base -->
    <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
        <rosparam file="$(find farm_nav)/param/costmap_common_params.yaml" command="load" ns="global_costmap" />
        <rosparam file="$(find farm_nav)/param/costmap_common_params.yaml" command="load" ns="local_costmap" />
        <rosparam file="$(find farm_nav)/param/local_costmap_params.yaml" command="load" />
        <rosparam file="$(find farm_nav)/param/global_costmap_params.yaml" command="load" />
        <rosparam file="$(find farm_nav)/param/base_local_planner_params.yaml" command="load" />
        <param name="planner_frequency" value="1" />
        <param name="controller_frequency" value="1" />
        <param name="controller_patience" value="15" />
        <param name="base_local_planner" value="regulated_pure_pursuit_controller/RegulatedPurePursuitController" />
    </node>

