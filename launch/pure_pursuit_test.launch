<!--         <param name="xmax" value="5.0" /> -->
<!--         <param name="ymin" value="-5.0" /> -->
<!--         <param name="ymax" value="5.0" /> -->
<!--         <param name="delta" value="0.01" /> -->
<!--         <param name="srr" value="0.1" /> -->
<!--         <param name="srt" value="0.5" /> -->
<!--         <param name="str" value="0.5" /> -->
<!--         <param name="stt" value="1.0" /> -->
<!--     </node> -->

    <node pkg="farm_nav" type="tf_manager.py" name="tf_manager"/>

    <!--   map_server -->
    <node pkg="map_server" type="map_server" name="map_server" args="$(find farm_nav)/map/map.yaml"/>

    <!--   als -->
    <include file="$(find als_ros)/launch/mcl_patrol_robot.launch" />

    <!--   rviz -->
<!--     <node pkg="rviz" type="rviz" name="rviz" args="-d $(find patrol_robot)/rviz_cfg/gmapping.rviz"/> -->
    <node pkg="rviz" type="rviz" name="rviz" args="-d $(find patrol_robot)/rviz_cfg/task_nav.rviz"/>
</launch> 

